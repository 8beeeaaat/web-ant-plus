{
  "name": "web-ant-plus",
  "version": "2.1.0",
  "description": "A package for ANT+ on Web browsers.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "example": "example"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
